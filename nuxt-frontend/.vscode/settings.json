{
  "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true,
  },
  "eslint.validate": [
      "javascript",
      "javascriptreact",
      "vue",
  ],
  "eslint.alwaysShowStatus": true,
  "eslint.workingDirectories": [
      "./"
  ],
  "editor.quickSuggestions": {
    "strings": true,
  },
  "debug.javascript.usePreview": false
}
